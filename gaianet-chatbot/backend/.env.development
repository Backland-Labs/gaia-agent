# Development Environment Configuration
# Based on specs/configuration-optimization.md:136-145

# GaiaNet Connection
GAIANET_BASE_URL=http://localhost:8080
GAIANET_API_KEY=dev-key-placeholder
GAIANET_MODEL=default

# Performance Settings - Optimized for development
CHAT_CTX_SIZE=8192
CHAT_BATCH_SIZE=32
EMBEDDING_CTX_SIZE=4096
EMBEDDING_BATCH_SIZE=16

# Security Settings - Relaxed for development
RATE_LIMIT_PER_HOUR=1000
MAX_MESSAGE_LENGTH=10000
ENABLE_DATA_PRIVACY_FILTER=true

# Server Settings
SERVER_HOST=127.0.0.1
SERVER_PORT=8080
FLASK_ENV=development

# Debug Settings
DEBUG_MODE=true
VERBOSE_LOGGING=true